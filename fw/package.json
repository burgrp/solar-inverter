{
  "name": "t100-dcmotor",
  "version": "1.0.0",
  "author": "Pavel Burgr",
  "license": "CC BY-SA",
  "dependencies": {
    "@si14/si-application-events": "^1.0.2",
    "@si14/si-atsamd-i2c": "^1.0.7",
    "@si14/si-atsamd-safeboot": "^1.0.2",
    "@si14/si-atsamd11c14a": "^2.0.4",
    "@si14/si-systick-timer": "^1.0.4"
  },
  "devDependencies": {
    "@si14/silicon": "2.0.4"
  },
  "scripts": {
    "build": "node_modules/.bin/silicon build --optimize g --size",
    "openocd": "openocd -f interface/cmsis-dap.cfg -f target/at91samdXX.cfg",
    "flash": "node_modules/.bin/silicon build --optimize g --size --flash",
    "dev": "node_modules/.bin/silicon build --optimize g --size --flash --loop",
    "generate": "node generate.js >src/generated.cpp"
  },
  "silicon": {
    "sources": [
      "src/adc.cpp",
      "src/gate.cpp",
      "src/uplink.cpp",
      "src/main.cpp"
    ],
    "symbols": {
      "ROM_START": "0x00000000",
      "RAM_START": "0x20000000",
      "RAM_SIZE": "4096"
    }
  }
}
